// 과목 등록
###
POST {{serverUrl}}/subjects
Authorization: Bearer {{accessToken}}
Content-Type: application/json

{
  "subjectName": "subjectName",
  "professorName": "professorName",
  "completion": "completion",
  "subjectCode": {{subjectCode}},
  "department": "department",
  "time": 1,
  "credit": 1
}

